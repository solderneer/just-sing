{
  "name": "just-sing-server",
  "version": "1.0.0",
  "description": "An Express.js server to power the just-sing backend",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rimraf dist/ && babel ./ --out-dir dist/ --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log --copy-files",
    "start": "npm run build && ./node_modules/nodemon/bin/nodemon.js dist/src/index.js --exec 'npm run lint && node'",
    "lint": "./node_modules/.bin/eslint src/**/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/solderneer/just-sing.git"
  },
  "author": "Sudharshan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/solderneer/just-sing/issues"
  },
  "homepage": "https://github.com/solderneer/just-sing#readme",
  "dependencies": {
    "body-parser": "1.18.3",
    "cors": "2.8.4",
    "express": "4.16.3",
    "morgan": "1.9.0",
    "rimraf": "2.6.2"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "eslint": "5.4.0",
    "eslint-config-standard": "11.0.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-node": "7.0.1",
    "eslint-plugin-promise": "4.0.0",
    "eslint-plugin-standard": "3.1.0",
    "nodemon": "1.18.3"
  }
}
